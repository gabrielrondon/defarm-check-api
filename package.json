{
  "name": "@defarm/check-api",
  "version": "1.0.0",
  "description": "API de compliance socioambiental para verificação de produtores e propriedades",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:worker": "tsx watch src/worker/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "worker": "node dist/worker/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx scripts/seed-sources.ts",
    "update:checker-dates": "tsx scripts/update-checker-dates.ts",
    "create-api-key": "tsx scripts/create-api-key.ts",
    "data:lista-suja": "curl -L 'https://www.gov.br/trabalho-e-emprego/pt-br/assuntos/inspecao-do-trabalho/areas-de-atuacao/cadastro_de_empregadores.xlsx' -o data/lista_suja.xlsx && tsx scripts/convert-lista-suja.ts",
    "data:ibama": "curl -L 'https://dadosabertos.ibama.gov.br/dados/SIFISC/termo_embargo/termo_embargo/termo_embargo_csv.zip' -o data/ibama_embargos.zip && cd data && unzip -o ibama_embargos.zip && cd .. && tsx scripts/convert-ibama-embargos.ts",
    "data:prodes": "tsx scripts/seed-prodes-sample.ts",
    "data:prodes-complete": "tsx scripts/download-prodes-complete.ts",
    "data:prodes-amazonia": "tsx scripts/download-prodes-complete.ts --biome=amazonia --years=5",
    "seed:prodes-complete": "tsx scripts/seed-prodes-complete.ts",
    "data:deter-daily": "tsx scripts/download-deter.ts",
    "data:deter-range": "tsx scripts/download-deter.ts",
    "seed:deter": "tsx scripts/seed-deter.ts",
    "data:funai-terras-indigenas": "tsx scripts/download-terras-indigenas.ts",
    "seed:terras-indigenas": "tsx scripts/seed-terras-indigenas.ts",
    "data:icmbio-unidades-conservacao": "tsx scripts/download-unidades-conservacao.ts",
    "seed:unidades-conservacao": "tsx scripts/seed-unidades-conservacao.ts",
    "data:car": "tsx scripts/download-car.ts",
    "data:car-all": "tsx scripts/download-car-all.ts",
    "data:car-optimized": "tsx scripts/download-car-priority-irregular.ts",
    "seed:car": "tsx scripts/seed-car.ts",
    "seed:car-all": "tsx scripts/seed-car-all.ts",
    "seed:car-optimized": "tsx scripts/seed-car-optimized.ts",
    "seed:car-v2": "tsx scripts/seed-car-optimized-v2.ts",
    "seed:car-large": "NODE_OPTIONS='--max-old-space-size=8192' tsx scripts/seed-car-optimized-v2.ts",
    "process:car": "tsx scripts/process-car-shapefiles.ts",
    "split:car": "tsx scripts/split-large-car-files.ts",
    "split:car:fast": "bash scripts/split-car-fast.sh",
    "data:cgu-sancoes": "tsx scripts/download-cgu-sancoes.ts",
    "seed:cgu-sancoes": "tsx scripts/seed-cgu-sancoes.ts",
    "data:queimadas": "tsx scripts/download-queimadas.ts",
    "seed:queimadas": "tsx scripts/seed-queimadas.ts",
    "data:mapa-organicos": "tsx scripts/download-mapa-organicos.ts",
    "seed:mapa-organicos": "tsx scripts/seed-mapa-organicos.ts",
    "data:mapbiomas-alerta": "tsx scripts/download-mapbiomas-alerta.ts",
    "data:mapbiomas-alerta-recent": "tsx scripts/download-mapbiomas-alerta.ts --months=6 --offset=0 --output=mapbiomas_alerta_recent.json",
    "data:mapbiomas-alerta-older": "tsx scripts/download-mapbiomas-alerta.ts --months=6 --offset=6 --output=mapbiomas_alerta_older.json",
    "data:mapbiomas-alerta-merge": "node -e \"const fs=require('fs'); const r=JSON.parse(fs.readFileSync('data/mapbiomas_alerta_recent.json')); const o=JSON.parse(fs.readFileSync('data/mapbiomas_alerta_older.json')); fs.writeFileSync('data/mapbiomas_alerta.json', JSON.stringify([...r,...o], null, 2)); console.log('Merged:', r.length + o.length, 'alerts')\"",
    "seed:mapbiomas-alerta": "tsx scripts/seed-mapbiomas-alerta.ts",
    "data:ana-outorgas": "tsx scripts/download-ana-outorgas.ts",
    "seed:ana-outorgas": "tsx scripts/seed-ana-outorgas.ts",
    "cron:setup": "bash scripts/cron/setup-cron.sh",
    "cron:test-deter": "tsx scripts/cron/update-deter.ts",
    "cron:test-prodes": "tsx src/worker/jobs/update-prodes.ts",
    "cron:test-lista-suja": "tsx scripts/cron/update-lista-suja.ts",
    "cron:test-ibama": "tsx scripts/cron/update-ibama.ts",
    "cron:test-spatial": "tsx scripts/cron/update-spatial-data.ts",
    "cron:test-car": "tsx scripts/cron/update-car.ts",
    "cron:test-cgu": "tsx src/worker/jobs/update-cgu-sancoes.ts",
    "cron:test-queimadas": "tsx src/worker/jobs/update-queimadas.ts",
    "cron:check-health": "tsx scripts/cron/check-data-freshness.ts",
    "test:telegram": "tsx scripts/test-telegram.ts",
    "backup:database": "tsx scripts/backup-database.ts",
    "benchmark:spatial": "tsx scripts/benchmark-spatial.ts",
    "seed:lista-suja-simple": "tsx scripts/seed-lista-suja-simple.ts",
    "seed:ibama-simple": "tsx scripts/seed-ibama-simple.ts",
    "seed:all-production": "tsx scripts/seed-all-production.ts",
    "data:all": "npm run data:lista-suja && npm run data:ibama && npm run data:prodes",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:e2e": "tsx scripts/test-e2e.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "compliance",
    "socioambiental",
    "deforestation",
    "defarm",
    "api"
  ],
  "author": "DeFarm",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.7.7",
    "bcryptjs": "^3.0.3",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.44.5",
    "fastify": "^4.29.1",
    "ioredis": "^5.4.1",
    "node-cron": "^4.2.1",
    "pg": "^8.11.3",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "uuid": "^10.0.0",
    "winston": "^3.19.0",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8",
    "tsx": "^4.16.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.14.10",
    "@types/pg": "^8.11.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "drizzle-kit": "^0.31.4",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
